<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        style="@style/TitleStyle"
     android:text="设置中心"
        />
    <com.huwenkai.com.mobilesafe.View.SetingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.huwenkai.com.mobilesafe"
        android:id="@+id/siv_updata"
        android:layout_width="match_parent"
        android:background="@drawable/selector_reset_setup_bg"
        android:layout_height="wrap_content"
        mobilesafe:destitle="自动更新设置"
        mobilesafe:desoff="自动更新已开启"
        mobilesafe:deson="自动更新已关闭">

    </com.huwenkai.com.mobilesafe.View.SetingItemView>
    <com.huwenkai.com.mobilesafe.View.SetingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.huwenkai.com.mobilesafe"
        android:id="@+id/siv_address"
        android:background="@drawable/selector_reset_setup_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="电话归属地显示"
        mobilesafe:desoff="归属地显示已开启"
        mobilesafe:deson="归属地显示已关闭">

    </com.huwenkai.com.mobilesafe.View.SetingItemView>
    <com.huwenkai.com.mobilesafe.View.AddressStyleView
    android:id="@+id/asv_style"
        android:background="@drawable/selector_reset_setup_bg"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >

    </com.huwenkai.com.mobilesafe.View.AddressStyleView>
    <com.huwenkai.com.mobilesafe.View.AddressStyleView
        android:id="@+id/asv_loacltion"
        android:background="@drawable/selector_reset_setup_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
    </com.huwenkai.com.mobilesafe.View.AddressStyleView>
    <com.huwenkai.com.mobilesafe.View.SetingItemView
        xmlns:mobilesafe="http://schemas.android.com/apk/res/com.huwenkai.com.mobilesafe"
        android:id="@+id/siv_blacknumber"

        android:background="@drawable/selector_reset_setup_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        mobilesafe:destitle="黑名单拦截设置"
        mobilesafe:desoff="黑名单拦截已开启"
        mobilesafe:deson="黑名单拦截已关闭">
    </com.huwenkai.com.mobilesafe.View.SetingItemView>
</LinearLayout>